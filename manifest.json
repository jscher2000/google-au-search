{
	"name": "Google Australia (google.com.au) Search",
	"description": "Add google.com.au to your search engines (including search suggestions from google.com.au). Not an official Google add-on.",
	"version": "0.1",
	"manifest_version": 2,
	"applications":{
		"gecko": {
			"id": "google-au-search@jeffersonscher.com",
			"strict_min_version": "64.0"
		}
	},
	"developer": {
		"name": "Jefferson \"jscher2000\" Scher",
		"url": "https://github.com/jscher2000/"
	},
	"icons": {
		"32": "favicon_au_24.png"
	},
	"web_accessible_resources": [
		"favicon_au_24.png"
	],
  "chrome_settings_overrides": {
		"search_provider": {
			"name": "Google AU",
			"search_url": "https://www.google.com.au/search?q={searchTerms}",
			"suggest_url": "https://www.google.com.au/complete/search?client=firefox&q={searchTerms}"
		}
	}
}
